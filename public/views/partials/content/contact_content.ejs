<div class="contact-wrap">

  <div class="title-wrap">
    <h4>I'd love to hear from you</h4>
  </div>

  <form action="" method="post">
    <input type="hidden" name="_after" value="/contact">
    <input type="text" name="_honey" value="" style="display:none">
    <input type="hidden" name="_confirmation" value="<b>Thank you!</b> I love hearing from you.">
    <p>Your Name (required)<br />
      <span class="wpcf7-form-control-wrap your-name"><input type="text" name="name"></span></p>
    <p>Your Email (required)<br />
      <span class="wpcf7-form-control-wrap your-email"><input type="email" name="email"></span></p>
    <p>Subject<br />
      <span class="wpcf7-form-control-wrap your-subject"><input type="text" name="_subject"></span></p>
    <p>Your Message<br />
      <span class="wpcf7-form-control-wrap your-message"><textarea name="message"></textarea></span></p>
    <p><input type="submit" value="Send" /></p>
  </form>
</div>

